<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.typinggame.StartScreenController" spacing="20" style="-fx-background-color: orange;">
    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Typing Game" style="-fx-font-size: 24px;" />
    <Button fx:id="startButton" onAction="#startGame" prefWidth="130" text="Start" />
</VBox>
